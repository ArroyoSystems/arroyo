                        },
                        "row_group_size": {
                          "title": "Row Group Size",
                          "type": "integer"
                        }
                      },
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "title": "JSON",
                      "additionalProperties": false
                    }
                  ]
                },
